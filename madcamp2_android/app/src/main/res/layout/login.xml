<ScrollView android:fillViewport="true"
    android:id="@+id/rwbzfoub1inf"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFFFF"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/r6pcmh8wop4y"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FFB4E9F9"
            android:orientation="vertical"
            android:paddingTop="40dp"
            android:paddingBottom="64dp">

            <LinearLayout
                android:id="@+id/rlj8ctl58uwl"
                android:layout_width="146dp"
                android:layout_height="146dp"
                android:layout_gravity="center"
                android:background="@drawable/shadow"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/rbiq8nbngp77"
                    android:layout_width="134dp"                    android:layout_height="134dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="6dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_101" />
            </LinearLayout>

            <TextView
                android:id="@+id/rx01skzzr27c"
                android:layout_width="277dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginHorizontal="89dp"
                android:layout_marginTop="22dp"
                android:layout_marginBottom="22dp"
                android:text="Meetup And Tracking Explorer"
                android:textAlignment="center"
                android:textColor="#1C1C1C"
                android:textSize="18sp" />

            <LinearLayout
                android:id="@+id/r9np50dwxd4b"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginHorizontal="40dp"
                android:layout_marginBottom="16dp"
                android:background="#FF313131"
                android:orientation="vertical"></LinearLayout>

            <TextView
                android:id="@+id/rx9tbb543n5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="41dp"
                android:layout_marginBottom="13dp"
                android:text="아이디"
                android:textColor="#000000"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/UserId_Textfield"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:layout_marginHorizontal="32dp"
                android:layout_marginBottom="18dp"
                android:background="@drawable/sff000000sw1cr6bbfd3f7ff"
                android:orientation="vertical"
                android:hint="아이디..."
                android:paddingLeft="10dp"></EditText>

            <TextView
                android:id="@+id/ruw2krkiq1q"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="41dp"
                android:layout_marginBottom="13dp"
                android:text="비밀번호"
                android:textColor="#000000"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/Password_Textfield"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:layout_marginHorizontal="32dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/sff000000sw1cr6bbfd3f7ff"
                android:orientation="vertical"
                android:hint="비밀번호..."
                android:inputType="textPassword"
                android:paddingLeft="10dp"></EditText>

            <TextView
                android:id="@+id/password_comment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="53dp"
                android:layout_marginBottom="10dp"
                android:text=""
                android:textColor="#FF0000"
                android:textSize="10sp" />

            <LinearLayout
                android:id="@+id/LoginButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="45dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/cr12bff424242"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:paddingVertical="9dp">

                <TextView
                    android:id="@+id/rk1z9b42lam8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="MATE 계정으로 로그인"
                    android:textColor="#FFFFFF"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/RegisterButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="45dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/cr12bff424242"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:paddingVertical="9dp">

                <TextView
                    android:id="@+id/rkr9ileb1z5c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="회원가입"
                    android:textColor="#FFFFFF"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/r4f797h8gptz"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="51dp"
                android:layout_marginBottom="15dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/ruqrs8jg8o5o"
                    android:layout_width="92dp"
                    android:layout_height="1dp"
                    android:background="#FF313131"
                    android:orientation="vertical"></LinearLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1" />

                <TextView
                    android:id="@+id/r38m277ohdhg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="연동 로그인"
                    android:textColor="#000000"
                    android:textSize="12sp" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1" />

                <LinearLayout
                    android:id="@+id/rgl1urw34js9"
                    android:layout_width="92dp"
                    android:layout_height="1dp"
                    android:background="#FF313131"
                    android:orientation="vertical"></LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/rzsxf7eqwjb"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="83dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/r5sqedce5rzj"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/naver" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1" />

                <ImageView
                    android:id="@+id/GoogleLoginButton"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/google" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1" />

                <ImageView
                    android:id="@+id/r4i2ka2rdhzf"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/apple" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</ScrollView>
